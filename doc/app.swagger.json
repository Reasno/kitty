{
  "swagger": "2.0",
  "info": {
    "title": "A kitty app",
    "version": "1.0"
  },
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/login": {
      "put": {
        "operationId": "App_Login",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1UserLoginReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1UserLoginRequest"
            }
          }
        ],
        "tags": [
          "App"
        ]
      }
    }
  },
  "definitions": {
    "appv1Gender": {
      "type": "string",
      "enum": [
        "GENDER_UNKNOWN",
        "GENDER_MALE",
        "GENDER_FEMALE"
      ],
      "default": "GENDER_UNKNOWN"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "typeUrl": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "v1UserLoginReply": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64"
        },
        "userName": {
          "type": "string"
        },
        "wechat": {
          "type": "string"
        },
        "headImg": {
          "type": "string"
        },
        "gender": {
          "$ref": "#/definitions/appv1Gender"
        },
        "birthday": {
          "type": "string"
        },
        "mobile": {
          "type": "string"
        },
        "readTime": {
          "type": "integer",
          "format": "int32"
        },
        "deviceId": {
          "type": "string"
        },
        "os": {
          "type": "string"
        },
        "channel": {
          "type": "string"
        },
        "versionCode": {
          "type": "string"
        },
        "isDel": {
          "type": "boolean"
        },
        "inviteCode": {
          "type": "string"
        }
      }
    },
    "v1UserLoginRequest": {
      "type": "object",
      "properties": {
        "mobile": {
          "type": "string",
          "pattern": "(^$|^[\\d]{13}$)"
        },
        "code": {
          "type": "string"
        },
        "wechat": {
          "type": "string"
        },
        "os": {
          "type": "string"
        },
        "deviceId": {
          "type": "string"
        },
        "channel": {
          "type": "string"
        },
        "versionCode": {
          "type": "string"
        }
      }
    }
  }
}
